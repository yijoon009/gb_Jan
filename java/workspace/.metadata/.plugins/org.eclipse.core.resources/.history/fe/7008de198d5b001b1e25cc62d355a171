package bank;

public class Bank {
	//객체
	//돈
	int money = 100000;
	
	//입금
	public void deposit(int money) {
		this.money += money;
	}
	//출금
	public boolean withdraw(int money) {
		this.money -= money;
		if(this.money<0) {
			return false;
		}else {
			return true;
		}
	}
	//잔액조회
	public int balance() {
		return money;
	}
}
